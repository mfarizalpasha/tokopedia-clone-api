{
  "version": 2,
  "builds": [{ "src": "app.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/videos", "dest": "routes/videoRoutes.js" },
    { "src": "/products", "dest": "routes/productRoutes.js" },
